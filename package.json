{
  "name": "@studiomitte/slide-menu",
  "version": "1.3.0",
  "author": "Jakob Osterberger <jo@studiomitte.com>",
  "license": "MIT",
  "type": "module",
  "description": "A library agnostic multilevel page menu with a smooth slide effect based on CSS transitions",
  "browserslist": [
    "> 1%"
  ],
  "keywords": [
    "menu",
    "multi level",
    "navigation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/studiomitte/slide-menu.git"
  },
  "bugs": "https://github.com/studiomitte/slide-menu/issues",
  "main": "dist/slide-menu.js",
  "files": [
    "./*",
    "dist/*",
    "src/*"
  ],
  "devDependencies": {
    "eslint": "^9.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "scripts": {
    "build": "npx vite build --emptyOutDir",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint --max-warnings=0 'src/**/*.ts'",
    "watch": "npx vite --port 8080",
    "typecheck": "npx tsc"
  },
  "dependencies": {
    "custom-event-polyfill": "1.0.7"
  }
}
