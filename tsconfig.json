{
  "compilerOptions": {
    "lib": [
      "es2019",
      "dom"
    ],
    "target": "ES6",
    "noImplicitReturns": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "store",
    "dist",
    "docs"
  ]
}
